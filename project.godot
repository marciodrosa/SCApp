; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "SCAppState",
"language": "GDScript",
"path": "res://sc_app_state.gd"
}, {
"base": "Resource",
"class": "SCData",
"language": "GDScript",
"path": "res://sc_data.gd"
}, {
"base": "Resource",
"class": "SCPerson",
"language": "GDScript",
"path": "res://sc_person.gd"
}, {
"base": "Reference",
"class": "SCResult",
"language": "GDScript",
"path": "res://sc_result.gd"
}, {
"base": "Reference",
"class": "SCVotesCalculator",
"language": "GDScript",
"path": "res://sc_votes_calculator.gd"
}, {
"base": "Reference",
"class": "WAT",
"language": "GDScript",
"path": "res://addons/WAT/namespace.gd"
}, {
"base": "Node",
"class": "WATTest",
"language": "GDScript",
"path": "res://addons/WAT/test/test.gd"
} ]
_global_script_class_icons={
"SCAppState": "",
"SCData": "",
"SCPerson": "",
"SCResult": "",
"SCVotesCalculator": "",
"WAT": "",
"WATTest": ""
}

[WAT]

Test_Directory="res://tests"
Results_Directory="res://tests/results"
Test_Metadata_Directory="res://tests/metadata"
Tags=PoolStringArray(  )
Cache_Tests=true
Window_Size=Vector2( 1280, 720 )
Minimize_Window_When_Running_Tests=false
Port=6008
Display=8

[application]

config/name="SCApp"
config/icon="res://icon.png"

[autoload]

AppState="*res://sc_app_state.gd"

[editor_plugins]

enabled=PoolStringArray( "res://addons/WAT/plugin.cfg" )

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
